{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Boundary": {
    "Name": {
      "MaxLength": 200,
      "AllowWhiteSpace": false
    },
    "LookupKey": {
      "MaxLength": 200,
      "AllowWhiteSpace": false
    }
  },
  "ValidationCascadeMode": "Stop",
  "ClientApp": {
    "Name": {
      "MaxLength": 200,
      "AllowWhiteSpace": false,
      "InvalidChars": "\""
    },
    "LookupKey": {
      "MaxLength": 200,
      "AllowWhiteSpace": false
    }
  },
  "Role": {
    "Name": {
      "MaxLength": 200
    },
    "LookupKey": {
      "MaxLength": 200
    }
  },
  "Permission": {
    "Name": {
      "MaxLength": 200
    },
    "LookupKey": {
      "MaxLength": 200
    }
  }
}
